<div class="row">
    <div class="col-lg-12">
        <h2 class="page-header"><%= @drone.title %></h2>




        <h2>FILE guru</h2>
<% @drone.file_projects.each do |file| %>
  <p>
    <%= file.name %>
  </p>
<% end %>

<% @drone.uploads.each do |file| %>
  <p>
    <%= file.uploaded_file_file_name %>
  </p>
 <script>

     window.onload =$( document ).ready (function() {
       thingiurlbase = "/js";
       thingiview = new Thingiview("viewer<%= file.uploaded_file_file_name %>");
       thingiview.setObjectColor('#C0D8F0');
       thingiview.initScene();

       thingiview.loadSTL("<%=file.uploaded_file.url(:original)%>");
     });
   </script>

   <div id="viewer<%= file.uploaded_file_file_name %>" style="width:300px;height:300px"></div>

<% end %>

    </div>
</div>



